@model IList<w1Consultorio.Models.ViewModel.GrupoViewModel>
@{
    Layout = null;
}

@for (int j = 0; j <= Model.Count() - 1; j++)
{
    <div class="panel panel-primary">
        <div class="panel-heading">@Html.DisplayFor(modelItem => modelItem[j].GrupoDescricao)</div>
        <div class="panel-body">
            <table>
                @for (int i = 0; i <= Model[j].Perguntas.Count - 1; i++)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => modelItem[j].Perguntas[i].PerguntaDescricao)
                        </td>
                        <td>
                            @Html.HiddenFor(modelItem => modelItem[j].GrupoSequencia)
                            @Html.HiddenFor(modelItem => modelItem[j].Perguntas[i].CodPergunta)
                            @Html.TextBoxFor(modelItem => modelItem[j].Perguntas[i].RespostaValor, new { @class = "form-control" })
                        </td>
                    </tr>

                }
            </table>
        </div>
    </div>
}